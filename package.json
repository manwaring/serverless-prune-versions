    "@types/node": "^14.6.4",
    "babel-jest": "^26.3.0",
    "codecov": "^3.7.2",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "publish-please": "^5.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
